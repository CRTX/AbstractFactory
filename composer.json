{
    "name": "CRTX/AbstractFactory",
    "description": "Generic Abstract Factory",
    "license": "MIT",
    "keywords": ["Factory", "Abstract"],
    "version": "2.0",
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~6",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CRTX\\AbstractFactory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CRTX\\AbstractFactory\\Test\\": "test/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
